{% if pageCount > 1 %}
    <ul class="pagination">
        {% for page in pagesInRange %}
            {% if page != current %}
                <li class="page">
                    <a href="{{ path(routeName, {page: page}) }}">{{ page }}</a>
                </li>
            {% else %}
                <li class="current">{{ page }}</li>
            {% endif %}
        {% endfor %}
    </ul>
{% endif %}
