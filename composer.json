{
    "name": "xelbot/telebot-auto-notes",
    "license": "Zlib",
    "type": "project",
    "description": "Telegram bot for autonotes",
    "require": {
        "php": ">=8.2",
        "ext-openssl": "*",
        "ext-pdo_mysql": "*",
        "ext-simplexml": "*",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "*",
        "doctrine/doctrine-migrations-bundle": "*",
        "doctrine/orm": "*",
        "google/protobuf": "^4.29",
        "guzzlehttp/guzzle": "^7.9",
        "guzzlehttp/psr7": "^2.7",
        "psr/log": "^3.0",
        "symfony/config": "^6.4",
        "symfony/dependency-injection": "^6.4",
        "symfony/dotenv": "^6.4",
        "symfony/form": "^6.4",
        "symfony/framework-bundle": "^6.4",
        "symfony/http-foundation": "^6.4",
        "symfony/http-kernel": "^6.4",
        "symfony/monolog-bundle": "^3.10",
        "symfony/routing": "^6.4",
        "symfony/security-bundle": "^6.4",
        "symfony/translation": "^6.4",
        "symfony/twig-bundle": "^6.4",
        "symfony/uid": "^6.4",
        "symfony/validator": "^6.4",
        "symfony/web-profiler-bundle": "^6.4",
        "symfony/yaml": "^6.4",
        "twirp/twirp": "^0.12.0",
        "xelbot/lib-telegram": "dev-master"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.39",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpunit/phpunit": "^11.5",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "AutoNotes\\": "generated/AutoNotes",
            "TeleBot\\": "src",
            "Tests\\": "tests/"
        }
    },
    "conflict": {
        "symfony/cache": ">=7.0",
        "symfony/clock": ">=7.0",
        "symfony/doctrine-bridge": ">=7.0",
        "symfony/error-handler": ">=7.0",
        "symfony/event-dispatcher": ">=7.0",
        "symfony/filesystem": ">=7.0",
        "symfony/finder": ">=7.0",
        "symfony/monolog-bridge": ">=7.0",
        "symfony/options-resolver": ">=7.0",
        "symfony/password-hasher": ">=7.0",
        "symfony/process": ">=7.0",
        "symfony/property-access": ">=7.0",
        "symfony/property-info": ">=7.0",
        "symfony/security-core": ">=7.0",
        "symfony/security-csrf": ">=7.0",
        "symfony/security-http": ">=7.0",
        "symfony/stopwatch": ">=7.0",
        "symfony/string": ">=7.0",
        "symfony/var-dumper": ">=7.0",
        "symfony/var-exporter": ">=7.0"
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    }
}
